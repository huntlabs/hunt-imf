    "configurations":[
        {
            "name" : "hunt-imf",
            "targetType": "library"
    
        },
        {
            "targetName": "helloworld",
            "name": "helloworld",
            "targetType": "executable",
            "sourceFiles": [
                "./example/helloworld/bootstrap.d",
                "./example/helloworld/command.d",
                "./example/helloworld/helloworld.d"
            ]
        },
        {
            "targetName": "chatclient",
            "name": "chatclient",
            "targetType": "executable",
            "sourceFiles": [
                "./example/chatroom/chatroom.d",
                "./example/chatroom/command.d",
                "./example/chatroom/client/client.d"
            ]
        },
        {
            "targetName": "chatserver",
            "name": "chatserver",
            "targetType": "executable",
            "sourceFiles": [
                "./example/chatroom/chatroom.d",
                "./example/chatroom/command.d",
                "./example/chatroom/server/server.d"
            ]
        }
    ]
